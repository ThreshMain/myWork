slick.slick('slickSetOption','slidesToScroll',slidesToScroll);topRelated.on('beforeChange',function(event,slickParam,currentSlide,nextSlide){if(nextSlide===0){prevArrow.style.visibility='hidden';}else{prevArrow.style.visibility='visible';}});},1500);}
asyncLoadScript('https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js',setUpSlick);trackPlays(gameSlug);attachReadMore(gameSlug);setupExitAnalyticsEvent();});